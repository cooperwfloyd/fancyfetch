{
  "name": "@cooperwfloyd/fancyfetch",
  "version": "0.13.7-beta",
  "type": "module",
  "license": "ISC",
  "main": "dist/index.js",
  "engines": {
    "node": ">=16.17.0 <19",
    "npm": ">=8.15.0 <10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cooperwfloyd/fancyfetch"
  },
  "dependencies": {
    "isomorphic-fetch": "^3.0.0",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.21",
    "eslint": "8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.8.0"
  },
  "scripts": {
    "build": "npm run test && npm run compile",
    "clean": "rm -rf ./dist && npm run test && npm run prettier",
    "compile": "swc ./src/* --out-dir ./dist",
    "prepublishOnly": "npm i && npm run build",
    "prepare": "husky install",
    "prettier": "npx prettier --write .",
    "start": "NODE_ENV='production' node --es-module-specifier-resolution=node \"./dist/test/index.js\"",
    "test": "npx eslint \"./src/**/*.js\" \"./src/*.js\" --max-warnings=0 --fix"
  }
}
